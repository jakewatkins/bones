{
  "GitHubFiles": {
    "BaseUrl": "https://raw.githubusercontent.com/jakewatkins/copilot-resources/main",
    "Files": [
      {
        "SourcePath": "copilot-instructions.md",
        "DestinationPath": "copilot-instructions.md",
        "IsRequired": true
      },
      {
        "SourcePath": ".editorconfig",
        "DestinationPath": ".editorconfig",
        "IsRequired": true
      },
      {
        "SourcePath": ".gitignore",
        "DestinationPath": ".gitignore",
        "IsRequired": true
      },
      {
        "SourcePath": "constitution.md",
