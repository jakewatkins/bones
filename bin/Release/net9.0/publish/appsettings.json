    "PromptFiles": [
      {
        "SourcePath": "prompts/analyze.prompt.md",
        "DestinationPath": ".github/prompts/analyze.prompt.md"
      },
      {
        "SourcePath": "prompts/clarify.prompt.md",
        "DestinationPath": ".github/prompts/clarify.prompt.md"
      }

    ]
  },
  "Directories": {
    "PersonalBase": "~/source/projects",
    "WorkBase": "~/source/csh"
  }
